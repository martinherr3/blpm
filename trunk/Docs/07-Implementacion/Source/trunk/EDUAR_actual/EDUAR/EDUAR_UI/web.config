        <add name="HttpPost" />
        <add name="HttpGet" />
        <add name="Documentation" />
        <add name="HttpPostLocalhost" />
      </protocols>
    </webServices>
    <httpRuntime requestValidationMode="2.0" />
    <sanitizer defaultProvider="AntiXssSanitizerProvider">
      <providers>
